package javaPrac;

public class ClassObjectMethods {

	int empId;
	String empName;
	int salary;
	int depNo;
	
	void Display()
	{
		System.out.println(empId);
		System.out.println(empName);
		System.out.println(salary);
		System.out.println(depNo);
	}
	
	public static void main(String[] args) {
		
		ClassObjectMethods Emp1=new ClassObjectMethods();
		Emp1.empId=1;
		Emp1.empName="Anmol";
		Emp1.salary=50000;
		Emp1.depNo=10;
		Emp1.Display();
		

	}

}
